{
    "presets": [
      [
        "env",
        {
          "loose": true,
          "modules": false,
          "targets": {
            "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
          }
        }
      ],
      "stage-2"
    ],
    "plugins": ["transform-vue-jsx", [
      "component",
      {
        "libraryName": "element-ui",
        // "styleLibraryName": "theme-chalk"
        "styleLibraryName": "~packages/theme-element/theme"
      }]
    ],
    "env": {
      "utils": {
        "presets": [
          [
            "env",
            {
              "loose": true,
              "modules": "commonjs",
              "targets": {
                "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
              }
            }
          ],
        ],
        "plugins": [
          ["module-resolver", {
            "root": ["runner-ui"],
            "alias": {
              "runner-ui/src": "runner-ui/lib"
            }
          }]
        ]
      },
      "test": {
        "plugins": ["istanbul"]
      }
    }
  }
  