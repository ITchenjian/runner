{
    "root":true,
    "globals": {
        "ga": true,
        "chrome": true
    },
    "plugins": ["html", "json"],
    "extends": "elemefe",
    "rules": {
        "indent": [4, "tab", {
            "SwitchCase": 1
        }],
        "no-restricted-globals": ["error", "event", "fdescribe"]
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true
        }
    }
}
  